{
    "format_version": "1.8.0",
    "render_controllers": {
      "controller.render.bedwars.player_armor": {
        "arrays": {
          "textures": {
            "array.visible": [
              "texture.visible"
            ],
            "array.invisible": [
              "texture.invisible"
            ]
          }
        },
        "geometry": "Geometry.default",
        "textures": [ "temp.life_time = query.life_time; return query.scoreboard('invisible_time') >= 1 ? array.invisible[temp.life_time] : array.visible[temp.life_time];", 
              "texture.enchanted" 
          ],
        "materials": [ { "*": "variable.is_enchanted ? material.enchanted : Material.default" } ]
      }
    }
  }